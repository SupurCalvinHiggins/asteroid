
load system io.
load "random".
load "util".
load "type".

let n = randint(0,10).
let guess = tointeger(io @input("Guess my number between 0 and 10: ")).
while guess =/= n do
    if guess < n do
        io @println "Too small.".
    elif guess > n  do
         io @println "Too big.".
    end
    let guess = tointeger(io @input("Try again: ")).
end
io @println "Yes, this is it!".
